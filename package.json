{
  "name": "express-api",
  "version": "1.0.0",
  "main": "index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "start": "node ./build/src/app.js",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts",
    "lint:check": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,json}'",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "prisma:init": "npx prisma init",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:push": "npx prisma db push",
    "seed": "npx prisma db seed",
    "erd": "npx prisma migrate",
    "postbuild": "copyfiles -u 2 src/locales/*.json build/src/locales && copyfiles -u 2 src/deep-link/*.json build/src/deep-link"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.696.0",
    "@aws-sdk/lib-storage": "^3.696.0",
    "@prisma/client": "^6.1.0",
    "@prisma/extension-read-replicas": "^0.4.0",
    "@types/body-parser": "^1.19.5",
    "@types/geoip-lite": "^1.4.4",
    "@types/hpp": "^0.2.6",
    "@types/i18n": "^0.13.12",
    "@types/jsdom": "^21.1.7",
    "@types/multer": "^1.4.12",
    "@types/randomstring": "^1.3.0",
    "@types/xss-filters": "^1.2.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.2.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "geoip-lite": "^1.4.10",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "i18n": "^0.15.1",
    "jsdom": "^25.0.1",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.46",
    "multer": "^1.4.5-lts.1",
    "prisma-erd-generator": "^1.11.2",
    "pushy": "^4.0.1",
    "randomstring": "^1.3.0",
    "uuid": "^11.0.3",
    "xss-filters": "^1.2.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prisma": "^6.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
